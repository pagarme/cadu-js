  },
  "dependencies": {
    "bluebird": "3.5.1",
    "crypto-js": "3.1.8",
    "joi": "12.0.0",
    "mappersmith": "2.9.1",
    "moment": "2.19.1",
    "ramda": "0.25.0"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.17.0",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.5",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-ramda": "1.4.3",
    "babel-preset-es2015": "6.16.0",
    "eslint": "4.8.0",
    "eslint-config-pagarme-base": "2.0.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "2.7.0",
    "husky": "0.14.3",
    "jest": "21.2.1",
    "json-loader": "0.5.7",
    "webpack": "1.13.2"
  }
